root = true

[*]
charset = utf-8
end_of_line = lf
insert_final_newline = true
trim_trailing_whitespace = true
max_line_length = 120

[scripts/*.zs]
indent_style = tab
tab_width = 4

[config/*.cfg]
indent_style = space
indent_width = 4

[config/agricraft/json/**/*.json]
indent_style = space
indent_width = 2
